    "bootstrap": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "eslint": "^8.56.0",
    "eslint-plugin-jest": "^27.6.3",
    "jest": "^29.2.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdoc": "^4.0.2",
    "jsdom": "^24.0.0",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.69.5"
  }
